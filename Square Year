        #include <stdio.h>
        #include <math.h>
     
        int main(){
            int t;
            scanf("%d", &t);
            while(t--){
                int s;
                scanf("%d", &s);
                int a = 0;
                int b = sqrt(s);
                if(b*b == s){
                    printf("%d %d\n", a, b);
                }else{
                    printf("-1\n");
                }
            }
        return 0;
        }
