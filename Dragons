        #include <stdio.h>
     
        int main(){
            int s, n;
            scanf("%d %d", &s, &n);
            int X[n], Y[n];
            int defeated = 0;
            for(int i =0; i <n; i++){
                scanf("%d %d", &X[i], &Y[i]);
            }
            for(int i =0; i <n; i++){
                if(s > X[i] && X[i] != 0){
                    s += Y[i];
                    X[i] = 0;
                    i = -1;
                    defeated++;
                }
            }
            if(n == defeated){
                printf("YES");
            }else{
                printf("NO");
            }
        return 0;
        }
