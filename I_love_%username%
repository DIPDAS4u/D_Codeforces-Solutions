            #include <stdio.h>
     
            int main(){
                int n;
                scanf("%d", &n);
                int points[n], count = 0;
                for(int i =0; i< n; i++){
                    scanf("%d", &points[i]);
                }
                int max = points[0];
                int min = points[0];
                for(int i = 1; i< n; i++){
                    if(points[i]< min){
                        count++;
                        min = points[i];
                    }
                    if(points[i]> max){
                        count++;
                        max = points[i];
                    }
                }
                printf("%d", count);
            return 0;
            }
