    #include <stdio.h>
     
    int main() {
        int n;
        scanf("%d", &n);
        char w[n];
        scanf("%s", w);
     
        int countA = 0;
        int countD = 0;
        for(int i = 0; w[i] != '\0'; i++) {
            if(w[i] == 'A') {
                countA++;
            }else if(w[i] == 'D'){
                countD++;
            }
        }
     
        if(countA > countD) {
            printf("Anton");
        } else if(countA < countD){
            printf("Danik");
        } else {
            printf("Friendship");
        }
     
        return 0;
    }
