            #include <stdio.h>
            #include <math.h>
            #define Max 1000000
     
            int prime[Max+1] = {0};
            void sieve(){
                for(int i = 2; i < Max; i++){
                    prime[i] = 1;
                }
                for(int i = 2; i*i <= Max; i++){
                        if(prime[i]){
                            for(int j = i*i; j <= Max; j += i){
                                prime[j] = 0;
                        }
                    }
                }
            }
     
            int main(){
                long long t;
                scanf("%lld", &t);
                sieve();
                while(t--){
                    long long n, i;
                    scanf("%lld", &n);
                    long long root = sqrt(n);
                    if( root*root == n && prime[root]){
                        printf("YES\n");
                    }else{
                        printf("NO\n");
                    }
                }
            return 0;
            }
