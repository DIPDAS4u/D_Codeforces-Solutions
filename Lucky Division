    #include <stdio.h>
     
    int main(){
        int n, flag = 0;
        scanf("%d", &n);
        while(n>0){
            if(n% 4 ==0 || n% 7 == 0 || n% 47 ==0 || n% 477 == 0){
                flag = 1;
                break;
            }
            break;
        }
        if(flag == 1){
            printf("YES");
        }else{
            printf("NO");
        }
        return 0;
    }
