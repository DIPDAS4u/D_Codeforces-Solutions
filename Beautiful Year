    #include <stdio.h>
     
    int uniqueDigits(int year){
        int digits[10] = {0};
        while(year > 0){
            int d = year % 10;
            if(digits[d] == 1){
                return 0;
            }else{
                digits[d] = 1;
                year = year/10;
            }
        }
        return 1;
    }
     
    int main(){
        int year;
        scanf("%d", &year);
        year++;
        while(!uniqueDigits(year)){
            year++;
        }
        printf("%d", year);
        return 0;
    }
